{
    "private": true,
    "nohoist": [
        "**/web",
        "**/web/**"
    ],
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "turbo": "1.6.2"
    },
    "dependencies": {
        "nps": "5.10.0"
    },
    "packageManager": "yarn@3.2.4",
    "version": "0.0.0"
}
